{
  "name": "plantify-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "storybook:start": "sb-rn-get-stories && STORYBOOK_ENABLED='true' expo start --dev-client",
    "storybook:android": "sb-rn-get-stories && STORYBOOK_ENABLED='true' expo start --android",
    "storybook:ios": "sb-rn-get-stories && STORYBOOK_ENABLED='true' expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "prepare": "husky install",
    "test": "jest",
    "build-ios": "eas build --profile development-simulator --platform ios --non-interactive",
    "build-android": "eas build --profile development --platform android",
    "build-all": "npm run build-ios && npm run build-android",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "expo": "~49.0.15",
    "expo-dev-client": "~2.4.12",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-svg": "13.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/slider": "^4.5.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.7",
    "@storybook/addon-ondevice-backgrounds": "^6.5.7",
    "@storybook/addon-ondevice-controls": "^6.5.7",
    "@storybook/addon-ondevice-notes": "^6.5.7",
    "@storybook/react-native": "^6.5.7",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.14",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "prettier": "^3.1.1",
    "react-native-safe-area-context": "^4.8.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
